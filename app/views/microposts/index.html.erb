<h1>メッセージ一覧</h1>
<table>
    <tr>
      <th>内容</th>
    </tr>
  <% @microposts.each do |micropost| %>
    <tr>
      <td><%= micropost.content %></td>
      <td><%= link_to '詳細を確認する',micropost_path(micropost.id) %></td>
    </tr>
  <% end %>
</table>
<%= link_to '新しくメッセージを投稿する',new_micropost_path %>
